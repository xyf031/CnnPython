
net: "SF/cifar10-alexnet/alexnet-cifar10.prototxt"

test_iter: 100
test_interval: 1000

base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 10000
momentum: 0.9
weight_decay: 0.001

display: 200
max_iter: 60000

snapshot: 10000
snapshot_prefix: "SF/cifar10-alexnet/alexnet_cifar10_train"

solver_mode: GPU
